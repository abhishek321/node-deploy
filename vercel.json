{
  
  "routes": [
    {
      "src": "/products",
      "dest": "index.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/add",
      "dest": "public/index.html"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    }
  ]
}